<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta charset="utf-8" />
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <meta name="author" content="PcMaxInf">
    <title>SintrafGV - Controle de acessos</title>
    @Styles.Render("~/Content/css")
    @RenderSection("scripts", required: false)
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAvLiOqGjJMQ2YHGKc1JCO9dGQNjrsjPTc&libraries=places" async defer></script>
    <script src="https://www.gstatic.com/firebasejs/5.10.1/firebase-app.js"></script>
    <script src="https://www.gstatic.com/firebasejs/5.10.1/firebase-messaging.js"></script>
    <script src="~/init-firebase.js"></script>
    
</head>
<body class="app header-fixed sidebar-fixed aside-menu-fixed sidebar-lg-show bg-gray-300">
    <div class="modal fade" id="mod-progress" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog">
    @*<div id="mod-progress" data-backdrop="static" data-keyboard="false" tabindex="-1" role="dialog">*@
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header bg-facebook">
                    <strong class="text-white"><h5>Aguarde!</h5></strong>
                </div>
                <div class="modal-body">
                    <div class="text-center">
                        <div class="">
                            <h1><i class="fa fa-cog fa-spin"></i></h1>
                        </div>
                        <div id="result" style="font-size: 1.1em; padding-bottom: 5px">
                            <p id="progressBarParagraph"></p>
                        </div>
                        <div style="width: 30%; margin: 0 auto; display:none;">
                            <div id="progressbar" style="width: 300px; height: 15px"></div>
                            <br />
                        </div>
                        <div id="ProgressStripe" class="progress progress-bar-animated active" style="position:relative; top:10px; width:100%;">
                            <div id="ProgressMessage" class="progress-bar" style="width: 100%; border:none;"></div>
                        </div>
                        <br />
                    </div>
                </div>
            </div>
        </div>
    </div>
    @RenderBody()
</body>
</html>
